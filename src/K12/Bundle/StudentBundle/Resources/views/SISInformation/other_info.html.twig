{% extends 'KulaCoreFrameworkBundle::sis.html.twig' %}


{% block content %}
<div class="group-box">
<div class="group-box-header">Other Info</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, field: 'K12.Student.ConversionStudentNumber', db_row_id: student.STUDENT_ID, value: student.CONV_STUDENT_NUMBER, label: true}) }}
</div>
</div>
<div class="group-box">
<div class="group-box-header">Notes</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, field: 'Core.Constituent.Notes', db_row_id: student.STUDENT_ID, value: student.NOTES, label: true, attributes_html: { rows: 20 }}) }}
</div>
</div>
{% endblock %}