{% extends 'KulaFrameworkBundle::sis.html.twig' %}

{% block content %}
<div class="data-table-header-div">
Levels
<span class="data-table-header-buttons">
	{{ kula_table_add('window_window_num_data_table_levels', 'STUD_SCHOOL_TERM_LEVEL') }}
</span>
</div>
<table id="window_window_num_data_table_levels" class="data-table">
<thead>
	<tr>
	{{ kula_field_name({ delete: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'SCHOOL_TERM_LEVEL_ID', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
	<th class="data-table-cell-row-header">#</th>
	{{ kula_field_name({ db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'LEVEL', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
	{{ kula_field_name({ db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'MIN_FULL_TIME_HOURS', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
	{{ kula_field_name({ db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'MIN_PART_TIME_HOURS', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
	{{ kula_field_name({ db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'FTE_FULL_TIME_HOURS', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
	<th class="data-table-cell-row-header">Actions</th>
	</tr>
</thead>
<tbody>
  {{ kula_display_html({ add: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'SCHOOL_TERM_LEVEL_ID', prepend_html: '<tr class="data-table-row-new">' }) }}
	{{ kula_field({ add: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'ORGANIZATION_TERM_ID', value: record_obj.getSelectedRecord.ORGANIZATION_TERM_ID, hidden: true }) }}
	{{ kula_field({ add: true, delete: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'SCHOOL_TERM_LEVEL_ID', prepend_html: '<td class="data-table-cell-row-delete">', append_html: '</td>' }) }}
	{{ kula_display_html({ add: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'SCHOOL_TERM_LEVEL_ID', prepend_html: '<td class="data-table-cell-row-num">#</td>' }) }}
	{{ kula_field({ add: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'LEVEL', prepend_html: '<td>', append_html: '</td>', attributes_html : { class : 'data-table-cell-input' } }) }}
	{{ kula_field({ add: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'MIN_FULL_TIME_HOURS', prepend_html: '<td>', append_html: '</td>', attributes_html : { class : 'data-table-cell-input' } }) }}
	{{ kula_field({ add: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'MIN_PART_TIME_HOURS', prepend_html: '<td>', append_html: '</td>', attributes_html : { class : 'data-table-cell-input' } }) }}
	{{ kula_field({ add: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'FTE_FULL_TIME_HOURS', prepend_html: '<td>', append_html: '</td>', attributes_html : { class : 'data-table-cell-input' } }) }}
		{{ kula_display_html({ add: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'SCHOOL_TERM_LEVEL_ID', prepend_html: '<td class="data-table-cell-row-num">Actions</td>' }) }}
	{{ kula_display_html({ add: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'SCHOOL_TERM_LEVEL_ID', prepend_html: '</tr>' }) }}
	{% for level in levels %}
	<tr>
		{{ kula_field({ delete: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'SCHOOL_TERM_LEVEL_ID', db_row_id: level.SCHOOL_TERM_LEVEL_ID, prepend_html: '<td class="data-table-cell-row-delete">', append_html: '</td>' }) }}
		<td class="data-table-cell-row-num">{{ loop.index }}</td>
		{{ kula_field({ edit: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'LEVEL', db_row_id: level.SCHOOL_TERM_LEVEL_ID, value: level.LEVEL, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
		{{ kula_field({ edit: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'MIN_FULL_TIME_HOURS', db_row_id: level.SCHOOL_TERM_LEVEL_ID, value: level.MIN_FULL_TIME_HOURS, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
		{{ kula_field({ edit: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'MIN_PART_TIME_HOURS', db_row_id: level.SCHOOL_TERM_LEVEL_ID, value: level.MIN_PART_TIME_HOURS, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
		{{ kula_field({ edit: true, db_table: 'STUD_SCHOOL_TERM_LEVEL', db_field: 'FTE_FULL_TIME_HOURS', db_row_id: level.SCHOOL_TERM_LEVEL_ID, value: level.FTE_FULL_TIME_HOURS, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
		<td>
			<a class="normal-link"  data-record-id="{{ record_obj.getSelectedRecordID() }}" data-record-type="{{ record_obj.getRecordType() }}" href="{{ path('sis_setup_schoolsetup_levels_fte', { level_id: level.SCHOOL_TERM_LEVEL_ID }) }}">FTE</a>
		</td>
	</tr>
	{% endfor %}
</tbody>
</table>
{% endblock %}