{% extends 'KulaCoreFrameworkBundle::sis.html.twig' %}

{% block detail %}
<div class="group-box">
<div class="group-box-header">Address Type</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, field: 'Core.Constituent.Address.Type', db_row_id: address.ADDRESS_ID, value: address.ADDRESS_TYPE, label: true }) }}
</div>
</div>
<div class="group-box">
<div class="group-box-header">Address</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, field: 'Core.Constituent.Address.Thoroughfare', db_row_id: address.ADDRESS_ID, value: address.THOROUGHFARE, label: true }) }}
</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, field: 'Core.Constituent.Address.Locality', db_row_id: address.ADDRESS_ID, value: address.LOCALITY, label: true }) }}
{{ kula_field({ edit: true, field: 'Core.Constituent.Address.AdministrativeArea', db_row_id: address.ADDRESS_ID, value: address.ADMINISTRATIVE_AREA, lookup: 'C', label: true }) }}
{{ kula_field({ edit: true, field: 'Core.Constituent.Address.PostalCode', db_row_id: address.ADDRESS_ID, value: address.POSTAL_CODE, label: true }) }}
</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, field: 'Core.Constituent.Address.Country', db_row_id: address.ADDRESS_ID, value: address.COUNTRY, label: true }) }}
</div>
</div>
<div class="group-box">
<div class="group-box-header">Notes</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, field: 'Core.Constituent.Address.Notes', db_row_id: address.ADDRESS_ID, value: address.NOTES, label: true }) }}
</div>
</div>
<div class="group-box">
<div class="group-box-header">Audit</div>
<div class="group-box-contents group">
{{ kula_field({ field: 'Core.Constituent.Address.CreatedUserstamp', value: address.createduser_FIRST_NAME~' '~address.createduser_LAST_NAME, label: true }) }}
{{ kula_field({ field: 'Core.Constituent.Address.CreatedTimestamp', value: address.CREATED_TIMESTAMP, label: true }) }}
{{ kula_field({ field: 'Core.Constituent.Address.UpdatedUserstamp', value: address.updateduser_FIRST_NAME~' '~address.updateduser_LAST_NAME, label: true }) }}
{{ kula_field({ field: 'Core.Constituent.Address.UpdatedTimestamp', value: address.UPDATED_TIMESTAMP, label: true }) }}
</div>
</div>
{% endblock %}