{% extends 'KulaFrameworkBundle::sis.html.twig' %}

{% block content %}
<div class="data-table-header-div">
Staff Organization Terms
<span class="data-table-header-buttons">
	{{ kula_table_add('window_window_num_data_table_orgterms', 'STUD_STAFF_ORGANIZATION_TERMS') }}
</span>
</div>
<table id="window_window_num_data_table_orgterms" class="data-table">
<thead>
	<tr>
	{{ kula_field_name({ delete: true, db_field: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'STAFF_ORGANIZATION_TERM_ID', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
	<th class="data-table-cell-row-header">#</th>
	{{ kula_field_name({ edit: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'ORGANIZATION_TERM_ID', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
	{{ kula_field_name({ edit: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'CONVERSION', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
	{{ kula_field_name({ edit: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'TEACHER_GRADES_OPEN', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
	{{ kula_field_name({ edit: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'TEACHER_GRADES_CLOSE', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
  </tr>
</thead>
<tbody>
	{{ kula_display_html({ add: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'STAFF_ORGANIZATION_TERM_ID', prepend_html: '<tr class="data-table-row-new">' }) }}
	{{ kula_field({ add: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'STAFF_ID', value: record_obj.getSelectedRecord.STAFF_ID, hidden: true }) }}
	{{ kula_field({ add: true, delete: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'STAFF_ORGANIZATION_TERM_ID', prepend_html: '<td class="data-table-cell-row-delete">', append_html: '</td>' }) }}
	{{ kula_display_html({ add: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'STAFF_ORGANIZATION_TERM_ID', prepend_html: '<td class="data-table-cell-row-num">#</td>' }) }}
	{{ kula_field({ add: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'ORGANIZATION_TERM_ID', prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
	{{ kula_field({ add: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'CONVERSION', prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
	{{ kula_field({ add: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'TEACHER_GRADES_OPEN', prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
	{{ kula_field({ add: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'TEACHER_GRADES_CLOSE', prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
	{{ kula_display_html({ add: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'STAFF_ORGANIZATION_TERM_ID', prepend_html: '</tr>' }) }}
	{% for stafforgterm in stafforgterms %}
	<tr>
		{{ kula_field({ delete: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'ORGANIZATION_TERM_ID', db_row_id: stafforgterm.STAFF_ORGANIZATION_TERM_ID, prepend_html: '<td class="data-table-cell-row-delete">', append_html: '</td>' }) }}
		<td class="data-table-cell-row-num">{{ loop.index }}</td>
		{{ kula_field({ edit: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'ORGANIZATION_TERM_ID', db_row_id: stafforgterm.STAFF_ORGANIZATION_TERM_ID, value: stafforgterm.ORGANIZATION_TERM_ID, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
		{{ kula_field({ edit: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'CONVERSION', db_row_id: stafforgterm.STAFF_ORGANIZATION_TERM_ID, value: stafforgterm.CONVERSION, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
		{{ kula_field({ edit: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'TEACHER_GRADES_OPEN', db_row_id: stafforgterm.STAFF_ORGANIZATION_TERM_ID, value: stafforgterm.TEACHER_GRADES_OPEN, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
		{{ kula_field({ edit: true, db_table: 'STUD_STAFF_ORGANIZATION_TERMS', db_field: 'TEACHER_GRADES_CLOSE', db_row_id: stafforgterm.STAFF_ORGANIZATION_TERM_ID, value: stafforgterm.TEACHER_GRADES_CLOSE, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
	</tr>
	{% endfor %}
</tbody>
</table>
{% endblock %}