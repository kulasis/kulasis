<div class="group-box">
<div class="group-box-header">Add for Organization</div>
<div class="group-box-contents group">
{{ kula_field({ non: true, db_table: 'STUD_STUDENT_COURSE_HISTORY', db_field: 'ORGANIZATION_ID', value: drop_date, label: true }) }}
