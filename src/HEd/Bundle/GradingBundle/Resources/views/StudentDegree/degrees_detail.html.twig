	<th class="data-table-cell-row-header">#</th>
	{{ kula_field_name({ db_table: 'STUD_STUDENT_DEGREES_CONCENTRATIONS', db_field: 'CONCENTRATION_ID', field_name_override: 'Concentration Name', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
