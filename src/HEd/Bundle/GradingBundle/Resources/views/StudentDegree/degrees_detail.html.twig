	{{ kula_field({ add: true, hidden: true, db_table: 'STUD_STUDENT_DEGREES_CONCENTRATIONS', db_field: 'STUDENT_DEGREE_ID', value: student_degree_id }) }}
	{{ kula_field({ add: true, delete: true, db_table: 'STUD_STUDENT_DEGREES_CONCENTRATIONS', db_field: 'STUDENT_CONCENTRATION_ID', prepend_html: '<td class="data-table-cell-row-delete">', append_html: '</td>' }) }}
