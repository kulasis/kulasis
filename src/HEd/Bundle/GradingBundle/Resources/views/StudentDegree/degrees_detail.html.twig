		<td class="data-table-cell-row-num">{{ loop.index }}</td>
		{{ kula_field({ edit: true, db_table: 'STUD_STUDENT_DEGREES_CONCENTRATIONS', db_field: 'CONCENTRATION_ID', db_row_id: concentration.STUDENT_CONCENTRATION_ID, value: concentration.CONCENTRATION_ID, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
	</tr>
	{% endfor %}
</tbody>
</table>
{% endblock %}