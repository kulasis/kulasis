	{{ kula_field({ add: true, hidden: true, db_table: 'STUD_STUDENT_DEGREES_MINORS', db_field: 'STUDENT_DEGREE_ID', value: student_degree_id }) }}
	{{ kula_field({ add: true, delete: true, db_table: 'STUD_STUDENT_DEGREES_MINORS', db_field: 'STUDENT_MINOR_ID', prepend_html: '<td class="data-table-cell-row-delete">', append_html: '</td>' }) }}
	{{ kula_display_html({ add: true, db_table: 'STUD_STUDENT_DEGREES_MINORS', db_field: 'STUDENT_MINOR_ID', prepend_html: '<td class="data-table-cell-row-num">#</td>' }) }}
	{{ kula_field({ add: true, db_table: 'STUD_STUDENT_DEGREES_MINORS', db_field: 'MINOR_ID', prepend_html: '<td>', append_html: '</td>', attributes_html : { class : 'data-table-cell-input' } }) }}
	{{ kula_display_html({ add: true, db_table: 'STUD_STUDENT_DEGREES_MINORS', db_field: 'STUDENT_MINOR_ID', prepend_html: '</tr>' }) }}
	{% for minor in minors %}
	<tr>
		{{ kula_field({ delete: true, db_table: 'STUD_STUDENT_DEGREES_MINORS', db_field: 'STUDENT_MINOR_ID', db_row_id: minor.STUDENT_MINOR_ID, prepend_html: '<td class="data-table-cell-row-delete">', append_html: '</td>' }) }}
		<td class="data-table-cell-row-num">{{ loop.index }}</td>
		{{ kula_field({ edit: true, db_table: 'STUD_STUDENT_DEGREES_MINORS', db_field: 'MINOR_ID', db_row_id: minor.STUDENT_MINOR_ID, value: minor.MINOR_ID, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
	</tr>
	{% endfor %}
</tbody>
</table>

<div class="data-table-header-div">
Concentrations
