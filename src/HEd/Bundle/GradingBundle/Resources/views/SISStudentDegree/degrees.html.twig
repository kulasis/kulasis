{% extends 'KulaCoreFrameworkBundle::sis.html.twig' %}

{% block content %}
<div class="data-table-header-div">
Degrees
<span class="data-table-header-buttons">
  {{ kula_table_add('window_window_num_data_table_degrees', 'STUD_STUDENT_DEGREES') }}
</span>
</div>
<table id="window_window_num_data_table_degrees" class="data-table">
<thead>
  <tr>
  {{ kula_field_name({ delete: true, field: 'HEd.Student.Degree.ID', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
  <th class="data-table-cell-row-header">#</th>
  {{ kula_field_name({ field: 'HEd.Student.Degree.EffectiveDate', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
  {{ kula_field_name({ field: 'HEd.Student.Degree.DegreeID', field_name_override: 'Degree Name', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
  {{ kula_field_name({ field: 'HEd.Student.Degree.ExpectedCompletionTermID', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
  {{ kula_field_name({ field: 'HEd.Student.Degree.DegreeAwarded', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
  {{ kula_field_name({ field: 'HEd.Student.Degree.GraduationDate', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
  {{ kula_field_name({ field: 'HEd.Student.Degree.ConferredDate', prepend_html: '<th class="data-table-cell-row-header">', append_html: '</th>' }) }}
    <th class="data-table-cell-row-header">...</th>
  </tr>
</thead>
<tbody>
  {{ kula_display_html({ add: true, field: 'HEd.Student.Degree.ID', prepend_html: '<tr class="data-table-row-new">' }) }}
  {{ kula_field({ add: true, hidden: true, field: 'HEd.Student.Degree.StudentID', value: kula_core_record.getSelectedRecord.STUDENT_ID }) }}
  {{ kula_field({ add: true, delete: true, field: 'HEd.Student.Degree.ID', prepend_html: '<td class="data-table-cell-row-delete">', append_html: '</td>' }) }}
  {{ kula_display_html({ add: true, field: 'HEd.Student.Degree.ID', prepend_html: '<td class="data-table-cell-row-num">#</td>' }) }}
  {{ kula_field({ add: true, field: 'HEd.Student.Degree.EffectiveDate', value:"now"|date("m/d/Y") , prepend_html: '<td>', append_html: '</td>', attributes_html : { class : 'data-table-cell-input' } }) }}
  {{ kula_field({ add: true, field: 'HEd.Student.Degree.DegreeID', prepend_html: '<td>', append_html: '</td>', attributes_html : { class : 'data-table-cell-input' } }) }}
  {{ kula_field({ add: true, field: 'HEd.Student.Degree.ExpectedCompletionTermID', prepend_html: '<td>', append_html: '</td>', attributes_html : { class : 'data-table-cell-input' } }) }}
  {{ kula_field({ add: true, field: 'HEd.Student.Degree.DegreeAwarded', prepend_html: '<td>', append_html: '</td>', attributes_html : { class : 'data-table-cell-input' } }) }}
  {{ kula_field({ add: true, field: 'HEd.Student.Degree.GraduationDate', prepend_html: '<td>', append_html: '</td>', attributes_html : { class : 'data-table-cell-input' } }) }}
  {{ kula_field({ add: true, field: 'HEd.Student.Degree.ConferredDate', prepend_html: '<td>', append_html: '</td>', attributes_html : { class : 'data-table-cell-input' } }) }}
  {{ kula_display_html({ add: true, field: 'HEd.Student.Degree.ID', prepend_html: '<td class="data-table-cell-row-num">#</td>' }) }}
  {{ kula_display_html({ add: true, field: 'HEd.Student.Degree.ID', prepend_html: '</tr>' }) }}
  {% for degree in degrees %}
  <tr>
    {{ kula_field({ delete: true, field: 'HEd.Student.Degree.ID', db_row_id: degree.STUDENT_DEGREE_ID, prepend_html: '<td class="data-table-cell-row-delete">', append_html: '</td>' }) }}
    <td class="data-table-cell-row-num">{{ loop.index }}</td>
    {{ kula_field({ edit: true, field: 'HEd.Student.Degree.EffectiveDate', db_row_id: degree.STUDENT_DEGREE_ID, value: degree.EFFECTIVE_DATE, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
    {{ kula_field({ edit: true, field: 'HEd.Student.Degree.DegreeID', db_row_id: degree.STUDENT_DEGREE_ID, value: degree.DEGREE_ID, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
    {{ kula_field({ edit: true, field: 'HEd.Student.Degree.ExpectedCompletionTermID', db_row_id: degree.STUDENT_DEGREE_ID, value: degree.EXPECTED_COMPLETION_TERM_ID, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
    {{ kula_field({ edit: true, field: 'HEd.Student.Degree.DegreeAwarded', db_row_id: degree.STUDENT_DEGREE_ID, value: degree.DEGREE_AWARDED, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
    {{ kula_field({ edit: true, field: 'HEd.Student.Degree.GraduationDate', db_row_id: degree.STUDENT_DEGREE_ID, value: degree.GRADUATION_DATE, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
    {{ kula_field({ edit: true, field: 'HEd.Student.Degree.ConferredDate', db_row_id: degree.STUDENT_DEGREE_ID, value: degree.CONFERRED_DATE, prepend_html: '<td>', append_html: '</td>', attributes_html: { class : 'data-table-cell-input' } }) }}
    <td class="data-table-cell-row-detail"><a class="normal-link" data-record-id="{{ kula_core_record.getSelectedRecordID() }}" data-record-type="{{ kula_core_record.getRecordType() }}" href="{{ path('sis_HEd_student_coursehistory_degrees_detail', { id : kula_core_record.getSelectedRecord.STUDENT_ID, sub_id : degree.STUDENT_DEGREE_ID }) }}">...</a></td>
  </tr>
  {% endfor %}
</tbody>
</table>
{% endblock %}