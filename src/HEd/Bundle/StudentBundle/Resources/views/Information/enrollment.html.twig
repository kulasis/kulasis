{% extends 'KulaFrameworkBundle::sis.html.twig' %}


{% block content %}
<div class="group-box">
<div class="group-box-header">Enrollment Status Change</div>
<div class="group-box-contents group">
{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_ENROLLMENT_ACTIVITY', db_field: 'EFFECTIVE_DATE', value: effective_date, label: true}) }}
</div>
<div class="group-box-contents group">
	{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_ENROLLMENT_ACTIVITY', db_field: 'GRADE', label: true }) }}
	{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_ENROLLMENT_ACTIVITY', db_field: 'LEVEL', label: true }) }}
	{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_ENROLLMENT_ACTIVITY', db_field: 'RESIDENT', label: true }) }}
	{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_ENROLLMENT_ACTIVITY', db_field: 'FTE', label: true }) }}
	{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_ENROLLMENT_ACTIVITY', db_field: 'THESIS_STATUS', label: true }) }}
</div>
</div>

<div class="group-box">
<div class="group-box-header">Degrees Seeking</div>
<div class="group-box-contents group">
{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_ENROLLMENT_ACTIVITY', db_field: 'SEEKING_DEGREE_1_ID', STUDENT_ID: record_obj.getSelectedRecord()['STUDENT_ID'], label: true, field_name_override: 'Seeking Degree 1' }) }}
{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_ENROLLMENT_ACTIVITY', db_field: 'SEEKING_DEGREE_2_ID', STUDENT_ID: record_obj.getSelectedRecord()['STUDENT_ID'], label: true, field_name_override: 'Seeking Degree 2' }) }}
</div>
</div>
{% endblock %}