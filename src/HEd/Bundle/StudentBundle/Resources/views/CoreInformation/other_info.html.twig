{% extends 'KulaCoreFrameworkBundle::core.html.twig' %}


{% block content %}
<div class="group-box">
<div class="group-box-header">Other Info</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, field: 'HEd.Student.ConversionStudentNumber', db_row_id: student.STUDENT_ID, value: student.CONV_STUDENT_NUMBER, label: true}) }}
</div>
</div>
<div class="group-box">
<div class="group-box-header">Last College Attended</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, field: 'HEd.Student.LastCollegeAttended', db_row_id: student.STUDENT_ID, value: student.LAST_COLLEGE_ATTENDED, label: true}) }}
{{ kula_field({ edit: true, field: 'HEd.Student.FirstGenerationCollege', db_row_id: student.STUDENT_ID, value: student.FIRST_GENERATION_COLLEGE, label: true}) }}
</div>
</div>
<div class="group-box">
<div class="group-box-header">Internal Notes</div>
<div class="group-box-contents group">
<p>These are administrative, internal notes. They are not available through API clients (such as Drupal).</p>
{{ kula_field({ edit: true, field: 'Core.Constituent.Notes', db_row_id: student.STUDENT_ID, value: student.NOTES, label: true, attributes_html: { rows: 20 }}) }}
</div>
</div>
<div class="group-box">
<div class="group-box-header">Medical/Allergy</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, field: 'HEd.Student.MedicalNotes', db_row_id: student.STUDENT_ID, value: student.MEDICAL_NOTES, label: true}) }}
</div>
</div>
{% endblock %}