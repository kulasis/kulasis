{% extends 'KulaFrameworkBundle::sis.html.twig' %}

{% block content %}

<div class="group-box">
<div class="group-box-header">Add Constituent</div>
<div class="group-box-contents group">
{{ kula_field({ add: true, db_table: 'CONS_CONSTITUENT', db_field: 'LAST_NAME', table_row: false, label: true }) }}
{{ kula_field({ add: true, db_table: 'CONS_CONSTITUENT', db_field: 'FIRST_NAME', table_row: false, label: true }) }}
{{ kula_field({ add: true, db_table: 'CONS_CONSTITUENT', db_field: 'MIDDLE_NAME', table_row: false, label: true }) }}
{{ kula_field({ add: true, db_table: 'CONS_CONSTITUENT', db_field: 'SOCIAL_SECURITY_NUMBER', table_row: false, label: true }) }}
</div>
</div>

<div class="group-box">
<div class="group-box-header">Birth Info</div>
<div class="group-box-contents group">
{{ kula_field({ add: true, db_table: 'CONS_CONSTITUENT', db_field: 'GENDER', table_row: false, label: true }) }}
{{ kula_field({ add: true, db_table: 'CONS_CONSTITUENT', db_field: 'BIRTH_DATE', table_row: false, label: true }) }}
</div>
</div>

<div class="group-box">
<div class="group-box-header">Enrollment</div>
<div class="group-box-contents group">
{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_STATUS', db_field: 'GRADE', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, label: true }) }}
{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_STATUS', db_field: 'LEVEL', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, label: true }) }}
{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_STATUS', db_field: 'RESIDENT', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, label: true }) }}
{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_STATUS', db_field: 'ENTER_DATE', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, label: true }) }}
{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_STATUS', db_field: 'ENTER_CODE', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, label: true }) }}
</div>
</div>

<div class="group-box">
<div class="group-box-header">New Degree</div>
<div class="group-box-contents group">
{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_DEGREES', db_field: 'DEGREE_ID', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, field_name_override: 'Degree', label: true }) }}
{{ kula_field({ add: true, table_row: false, db_table: 'STUD_STUDENT_DEGREES', db_field: 'EXPECTED_COMPLETION_TERM_ID', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, field_name_override: 'Expected Completion Term', label: true }) }}
</div>
</div>

{% endblock %}