{% extends 'KulaFrameworkBundle::sis.html.twig' %}


{% block content %}
<div class="group-box">
<div class="group-box-header">Other Info</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, db_table: 'STUD_STUDENT', db_field: 'CONV_STUDENT_NUMBER', db_row_id: student.STUDENT_ID, value: student.CONV_STUDENT_NUMBER, label: true}) }}
</div>
</div>
<div class="group-box">
<div class="group-box-header">Last College Attended</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, db_table: 'STUD_STUDENT', db_field: 'LAST_COLLEGE_ATTENDED', db_row_id: student.STUDENT_ID, value: student.LAST_COLLEGE_ATTENDED, label: true}) }}
</div>
</div>
<div class="group-box">
<div class="group-box-header">Notes</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, db_table: 'CONS_CONSTITUENT', db_field: 'NOTES', db_row_id: student.STUDENT_ID, value: student.NOTES, label: true, attributes_html: { rows: 20 }}) }}
</div>
</div>
{% endblock %}