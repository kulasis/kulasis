{{ kula_field({ edit: true, db_table: 'CONS_CONSTITUENT', db_field: 'SOCIAL_SECURITY_NUMBER', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, value: student.STUDENT_ID, label: true  }) }}
