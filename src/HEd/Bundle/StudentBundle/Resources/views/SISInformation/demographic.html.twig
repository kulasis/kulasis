{% extends 'KulaFrameworkBundle::sis.html.twig' %}

{% block content %}
{% if mode == 'edit' %}
<div class="group-box">
<div class="group-box-header">Name</div>
<div class="group-box-contents group">
{{ kula_field({ edit: true, db_table: 'CONS_CONSTITUENT', db_field: 'LAST_NAME', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, value: record_obj.getSelectedRecord.LAST_NAME, label: true }) }}
{{ kula_field({ edit: true, db_table: 'CONS_CONSTITUENT', db_field: 'FIRST_NAME', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, value: record_obj.getSelectedRecord.FIRST_NAME, label: true  }) }}
{{ kula_field({ edit: true, db_table: 'CONS_CONSTITUENT', db_field: 'MIDDLE_NAME', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, value: record_obj.getSelectedRecord.MIDDLE_NAME, label: true  }) }}
{% if mode == 'edit' %}
{{ kula_field({ edit: true, db_table: 'CONS_CONSTITUENT', db_field: 'GENDER', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, value: record_obj.getSelectedRecord.GENDER, label: true, lookup: 'C'  }) }}
{% endif %}
{{ kula_field({ edit: true, db_table: 'CONS_CONSTITUENT', db_field: 'MAIDEN_NAME', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, value: record_obj.getSelectedRecord.MAIDEN_NAME, label: true  }) }}
{{ kula_field({ edit: true, db_table: 'CONS_CONSTITUENT', db_field: 'PREFERRED_NAME', db_row_id: record_obj.getSelectedRecord.STUDENT_ID, value: record_obj.getSelectedRecord.PREFERRED_NAME, label: true  }) }}
</div>
</div>
{% endif %}
