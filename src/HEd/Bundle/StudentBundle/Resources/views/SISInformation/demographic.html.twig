{{ kula_field({ edit: true, db_table: 'STUD_STUDENT', db_field: 'ORIG_ENTER_DATE', db_row_id: student.STUDENT_ID, value: student.ORIG_ENTER_DATE, label: true}) }}
{{ kula_field({ edit: true, db_table: 'STUD_STUDENT', db_field: 'ORIG_ENTER_CODE', db_row_id: student.STUDENT_ID, value: student.ORIG_ENTER_CODE, label: true}) }}
{{ kula_field({ edit: true, db_table: 'STUD_STUDENT', db_field: 'ORIG_ENTER_TERM', db_row_id: student.STUDENT_ID, value: student.ORIG_ENTER_TERM, label: true}) }}
</div>
</div>
